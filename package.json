{
  "name": "extatistic",
  "version": "1.3.5",
  "description": "Perform statistical models on any text and extract specific features",
  "main": "index.js",
  "bin": {
    "extract": "bin/cli.js"
  },
  "scripts": {
    "test": "cross-env ENV=test jest",
    "doc": "jsdoc index.js README.md -d doc -p package.json",
    "babel": "babel . --out-dir predist",
    "prebuild": "npm run babel && npm run doc",
    "build": "webpack predist/index.js -o dist/index.js -p",
    "cli": "webpack predist/bin/cli.js -o dist/bin/cli.js -p",
    "postpublish": "git clean -fd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardjs/extatistic.git"
  },
  "keywords": [
    "statistic",
    "language",
    "analysis",
    "textparser",
    "parser",
    "wordparser"
  ],
  "author": "easa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guardjs/extatistic/issues"
  },
  "homepage": "https://github.com/guardjs/extatistic#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-jest": "^25.1.0",
    "cross-env": "^7.0.0",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.4",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "node-excel-export": "^1.4.4",
    "yargs": "^15.1.0"
  }
}