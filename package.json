{
  "name": "extatistic",
  "version": "1.3.5",
  "description": "Perform statistical models on any text and extract specific features",
  "main": "index.js",
  "bin": {
    "extract": "bin/cli.js"
  },
  "scripts": {
    "test": "cross-env ENV=test jest",
    "doc": "jsdoc index.js README.md -d docs -p",
    "babel": "babel . --out-dir predist",
    "prebabel": "rm ./predist",
    "postbabel": "cp ./package.json ./predist/package.json",
    "prebuild": "npm run doc && npm run babel",
    "webpack": "webpack predist/index.js -o dist/index.js -p",
    "build": "npm run webpack",
    "commit": "git add . && git commit -m \"version update\"",
    "preversion": "npm test",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags && rm -rf build/temp",
    "shebang": "sed -i '#! /usr/bin/env node\n' dist/cli.js",
    "notused becuase it's not a library": "&& webpack predist/bin/cli.js -o dist/bin/cli.js -p && npm run shebang",
    "postpublish": "git clean -fd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardjs/extatistic.git"
  },
  "keywords": [
    "statistic",
    "language",
    "analysis",
    "textparser",
    "parser",
    "wordparser"
  ],
  "author": "easa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guardjs/extatistic/issues"
  },
  "homepage": "https://github.com/guardjs/extatistic#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-jest": "^25.1.0",
    "cross-env": "^7.0.0",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.4",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "node-excel-export": "^1.4.4",
    "yargs": "^15.1.0"
  }
}